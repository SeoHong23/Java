package test1;

/*
 * 날짜 : 2024/07/03
 * 이름 : 박서홍
 * 내용 : 자바 기본 입출력 연습문제
 */
public class Test03 {
	public static void main(String[] args) {
		
		int num = 0;
		
		result = num++;
		System.out.println("result : " + result);
		
		result = ++num;
		System.out.println("result : " + result);
		
		result = num--;
		System.out.println("result : " + result);
		
		result = --num;
		System.out.println("result : " + result);
		
	}
	

}
