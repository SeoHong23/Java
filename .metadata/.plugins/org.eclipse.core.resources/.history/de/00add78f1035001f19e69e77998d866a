 */
public class DateTypeTest {
	
	public static void main(String[] args) {
		
		// 정수형
		byte num1 = 127;
		short num2 = 32767;
		int num3 = 2147483647;
		long num4 = 9999999999L;
		
		System.out.println("num1 : " + num1);
		System.out.println("num2 : " + num2);
		System.out.println("num3 : " + num3);
		System.out.println("num4 : " + num4);
		
		// 실수형
		// 논리형
		// 문자형
		// 문자열
	}

}
